{
  "Function": {
    "prefix": "func",
    "body": [
      "function ${1:name}($2) {",
      "  $3",
      "}"
    ],
    "description": "快速创建Function函数"
  },
  "Arrow Function": {
    "prefix": "func",
    "body": [
      "const ${1:name} = ($2) => {",
      "  $3",
      "};"
    ],
    "description": "快速创建箭头函数"
  },
  "forEach": {
    "prefix": "foreach",
    "body": [
      "forEach((${1|item,element,child|}) => {",
      "  $2",
      "})$3;"
    ],
    "description": "快速创建forEach函数体"
  },
  "reduce": {
    "prefix": "reduce",
    "body": [
      "reduce((${1:result}, ${2|item,element,child|}) => {",
