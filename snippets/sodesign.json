{
  "TableList": {
    "prefix": "tablelist",
    "body": [
      "import React from 'react';",
      "import { TableList } from '@so-design/comps';",
      "import { Button, Space, Tooltip } from 'antd';",
      "import { getLocale, useRequest } from '@so-design/utils';",
      "import { useTranslation } from 'react-i18next';",
      "import { DeleteIcon } from '@sophon/icons';",
      "",
      "export default function Test() {",
      "  const locale = getLocale();",
      "  const { t } = useTranslation();",
      "  const {",
      "    runAsync: onLoadList,",
      "    loading,",
      "    data: dataSource,",
      "  } = useRequest(",
      "    async () => {",
      "      return [",
      "        {",
      "          id: '1',",
      "          name: 'John Br Blue Whiteown',",
      "          age: 32,",
      "        },",
      "      ];",
      "      // return await api.Test();",
      "    },",
      "    { manual: false }",
      "  );",
      "  const headRender = React.useCallback(() => {",
      "    return <Button type='primary'>Test</Button>;",
      "  }, [locale]);",
      "  const batchRender = React.useCallback(() => {",
      "    return (",
      "      <Tooltip title={t('OcrApp:Delete')}>",
      "        <DeleteIcon />",
      "      </Tooltip>",
      "    );",
      "  }, [locale]);",
      "  const columns = React.useMemo(() => {",
      "    return [",
      "      {",
      "        title: 'Name',",
      "        dataIndex: 'name',",
      "        key: 'name',",
      "      },",
      "      {",
      "        title: 'Age',",
      "        dataIndex: 'age',",
      "        search: true,",
      "        sorter: true,",
      "        width: '20%',",
      "      },",
      "      {",
      "        title: 'Operation',",
      "        dataIndex: 'operation',",
      "        render: () => {",
      "          return (",
      "            <Space>",
      "              <a>按钮</a>",
      "            </Space>",
      "          );",
      "        },",
      "      },",
      "    ];",
      "  }, [locale]);",
      "  return (",
      "    <TableList",
      "      rowKey='id'",
      "      showFullscreen",
      "      dataSource={dataSource}",
      "      columns={columns}",
      "      loading={loading}",
      "      refresh={onLoadList}",
      "      headRender={headRender}",
      "      batchRender={batchRender}",
      "    />",
      "  );",
      "}"
    ],
    "description": "快速创建TableList"
  }
}